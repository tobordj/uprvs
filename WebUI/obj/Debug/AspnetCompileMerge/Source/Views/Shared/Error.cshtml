@model System.Web.Mvc.HandleErrorInfo
@{
    ViewBag.Title = "УПР: раздел не найден";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div class="container">
    <div class="row">
        <div class="span12">
            <div class="stepmenu">
                <ul class="breadcrumb">
                    <li><a href="~/"><span title="Главная" class="icon  icon-home">​</span> Главная </a><span class="divider">/</span></li>
                    <li class="active"> Раздел не найден или не существует</li>
                </ul>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="span12">
            <div class="social-box">
                <div class="header"><span class="icon32  icon-arrowstop-e"></span> Раздел не найден или не существует </div>
            </div>

@if (Model != null && HttpContext.Current.IsDebuggingEnabled)
    {
    <div>
        <p>
            <b>Exception:</b> @Model.Exception.Message<br />
            <b>Controller:</b> @Model.ControllerName<br />
            <b>Action:</b> @Model.ActionName
        </p>
        <div style="overflow:scroll">
            <pre>
@Model.Exception.StackTrace

                </pre>
        </div>
    </div>
}
            <div class="a7">

                <br> 

            </div>

        </div>
    </div>
</div>